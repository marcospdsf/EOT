    <item name="backpack">
    <type class="container" subclass="backpack" tileid="-1"/>
    <description>You see a backpack.</description>
     <implementation class-name="games.stendhal.server.entity.item.Container"/>
    <attributes>
      <slot_name value="bag"/>
      <undroppableondeath value="1" />
      <autobind value="1" />
      <slot_size value="15"/>
    </attributes>
    <weight value="1.0"/>
    <value value="1"/>
    <equipable>
      <slot name="ground"/>
<!--       <slot name="content"/>  -->
      <slot name="back"/>
<!--      <slot name="bag"/>
      <slot name="trade"/>
 -->      
    </equipable>
  </item>
  
